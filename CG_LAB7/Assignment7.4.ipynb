   "source": [
    "def swap_list_elements(lst, idx1, idx2):\n",
    "    temp = lst[idx1]\n",
    "    lst[idx1] = lst[idx2]\n",
    "    lst[idx2] = temp\n",
    "    return lst\n",
    "\n",
    "my_list = [10, 20, 30, 40, 50]\n",
    "print(\"Before swap:\", my_list)\n",
    "print(\"After swap:\", swap_list_elements(my_list, 1, 3))"
   ]
  }
 ],
